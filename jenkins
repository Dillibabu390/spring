pipeline {
agent any

stages {
stage('Checkout') {
steps {
git branch: 'main', url: 'https://github.com/Dillibabu390/spring.git'
}
}


stage('Build') {
steps {
withEnv ( ['JENKINS_NODE_COOKIE=do_not_kill'] )
{
bat 'mvn --version'
bat 'java -version'
bat 'mvn clean install'
}
}
}


stage('Run') {
steps {
withEnv ( ['JENKINS_NODE_COOKIE=do_not_kill'] )
{
bat 'java -jar target/spring-0.0.1-SNAPSHOT.jar'
}
}
}

}
}
